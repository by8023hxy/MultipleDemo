<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baiyu.mytestdemo">

    <application
        android:allowBackup="true"
        android:name=".DemoApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".intentflag.TestIntentFlagActivityA" />
        <activity android:name=".intentflag.TestIntentFlagActivityB" />
        <activity android:name=".intentflag.TestIntentFlagActivityC" />
        <activity android:name=".intentflag.TestIntentFlagActivityD" />
        <activity android:name=".intentflag.TestIntentFlagActivityE" />
        <activity android:name=".intentflag.TestIntentFlagActivityF" />
    </application>

</manifest>